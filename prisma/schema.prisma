generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model games {
  id           Int    @id @default(autoincrement())
  title        String @db.VarChar(50)
  release_date String
}

model users {
  id       Int     @id @default(autoincrement())
  name     String  @db.VarChar(50)
  password String  @db.VarChar(60)
  email    String  @unique @db.VarChar(50)
  token    String?
}
